  /v1/me:
    get:
      tags:
        - Core
      summary: Get information about current user
      description: After token grant, get information of the token holder
      responses:
        '200':
          description: OK
          headers:
            Server:
              schema:
                type: number
                example: nginx/1.20.1
            Date:
              schema:
                type: string
                example: Thu, 27 Jan 2022 12:39:59 GMT
            Content-Type:
              schema:
                type: string
                example: application/json; charset=utf-8
            Content-Length:
              schema:
                type: integer
                example: '128'
            Connection:
              schema:
                type: string
                example: keep-alive
            Cache-Control:
              schema:
                type: string
                example: no-cache
            Etag:
              schema:
                type: string
                example: W/"546045afc1496b3349d71f14a2342321"
            Strict-Transport-Security:
              schema:
                type: string
                example: max-age=63072000; includeSubDomains
            Vary:
              schema:
                type: string
                example: Origin
            X-Controller-Path:
              schema:
                type: string
                example: api_public/v1/credentials#me
            X-Factorial-Node:
              schema:
                type: string
                example: NODE_ignacio-cifuentes-dev
            X-Request-Id:
              schema:
                type: string
                example: 3643b54e-b7e8-4722-b5cd-afcd50fba2f6
            X-Runtime:
              schema:
                type: number
                example: '0.091405'
          content:
            text/plain:
              schema:
                type: string
              example: |-
                {
                    "email": "bob@factorial.co",
                    "full_name": "Bob The Boss",
                    "first_name": "Bob",
                    "last_name": "The Boss",
                    "employee_id": 5,
                    "role": "basic"
                }
